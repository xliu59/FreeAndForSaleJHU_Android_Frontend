<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team16.oose_project">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".core.GuestHomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".registration.email.EmailRegistration" />
        <activity android:name=".registration.email.SuccessfulEmailRegistration" />
        <activity android:name=".registration.phone.PhoneRegistration" />
        <activity android:name=".registration.phone.SuccessfulPhoneRegistration" />
        <activity android:name=".registration.password.PasswordSetup" />
        <activity android:name=".registration.password.SuccessfulPasswordSetup" />
        <activity android:name=".registration.email.EmailCodeVerification" />
        <activity android:name=".registration.phone.PhoneCodeVerification" />
        <activity android:name=".login.Login" />
        <activity android:name=".items.ItemList" />
        <activity android:name=".MyAccount.MyAccountUpdate" />
        <activity android:name=".MyAccount.MyAccountDisplay"></activity>
    </application>

</manifest>