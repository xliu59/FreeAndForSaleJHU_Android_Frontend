<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#e1e1e1"
    tools:context="com.team16.oose_project.core.GuestHomePage">

    <ImageButton
        android:layout_width="175dp"
        android:layout_height="108dp"
        app:srcCompat="@drawable/kitchen"
        android:layout_below="@+id/bedroomButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/kitchenButton"
        android:scaleType="centerCrop" />

    <ImageButton
        android:layout_width="175dp"
        android:layout_height="108dp"
        app:srcCompat="@drawable/balcony"
        android:layout_below="@+id/bathroomButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/balconyButton"
        android:scaleType="centerCrop" />

    <ImageButton
        android:layout_width="175dp"
        android:layout_height="108dp"
        app:srcCompat="@drawable/children"
        android:layout_below="@+id/livingroomButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/childroomButton"
        android:scaleType="centerCrop" />

    <ImageButton
        android:layout_width="175dp"
        android:layout_height="108dp"
        app:srcCompat="@drawable/more"
        android:id="@+id/moreButton"
        android:scaleType="centerCrop"
        android:layout_below="@+id/balconyButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="Post"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/postButton"
        android:layout_below="@+id/childroomButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/background_light"
        android:background="@android:color/holo_blue_bright" />

    <Button
        android:text="Selling"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/childroomButton"
        android:layout_toRightOf="@+id/postButton"
        android:layout_toEndOf="@+id/postButton"
        android:id="@+id/sellingButton"
        android:background="@android:color/holo_blue_bright"
        android:textColor="@android:color/background_light" />

    <Button
        android:text="Wishes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/moreButton"
        android:layout_toRightOf="@+id/sellingButton"
        android:layout_toEndOf="@+id/sellingButton"
        android:id="@+id/wishesButton"
        android:background="@android:color/holo_blue_bright"
        android:textColor="@android:color/background_light" />

    <Button
        android:text="Me"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/moreButton"
        android:layout_toRightOf="@+id/wishesButton"
        android:layout_toEndOf="@+id/wishesButton"
        android:id="@+id/meButton"
        android:textColor="@android:color/background_light"
        android:background="@android:color/holo_blue_bright" />

    <ImageButton
        android:layout_width="175dp"
        app:srcCompat="@drawable/bedroom"
        android:id="@+id/bedroomButton"
        android:layout_height="108dp"
        android:scaleType="centerCrop"
        android:layout_below="@+id/action_bar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="175dp"
        app:srcCompat="@drawable/bedroom"
        android:id="@+id/imageButton16"
        android:layout_height="106dp"
        android:scaleType="centerCrop"
        android:layout_below="@+id/action_bar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="175dp"
        app:srcCompat="@drawable/bedroom"
        android:id="@+id/imageButton15"
        android:layout_height="106dp"
        android:scaleType="centerCrop"
        android:layout_below="@+id/action_bar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="175dp"
        app:srcCompat="@drawable/bedroom"
        android:id="@+id/imageButton14"
        android:layout_height="106dp"
        android:scaleType="centerCrop"
        android:layout_below="@+id/action_bar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="175dp"
        app:srcCompat="@drawable/bedroom"
        android:id="@+id/imageButton13"
        android:layout_height="106dp"
        android:scaleType="centerCrop"
        android:layout_below="@+id/action_bar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="175dp"
        app:srcCompat="@drawable/bedroom"
        android:id="@+id/imageButton12"
        android:layout_height="106dp"
        android:scaleType="centerCrop"
        android:layout_below="@+id/action_bar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="175dp"
        app:srcCompat="@drawable/bedroom"
        android:id="@+id/imageButton11"
        android:layout_height="106dp"
        android:scaleType="centerCrop"
        android:layout_below="@+id/action_bar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="175dp"
        app:srcCompat="@drawable/bedroom"
        android:id="@+id/imageButton10"
        android:layout_height="106dp"
        android:scaleType="centerCrop"
        android:layout_below="@+id/action_bar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="175dp"
        app:srcCompat="@drawable/bedroom"
        android:id="@+id/imageButton9"
        android:layout_height="108dp"
        android:scaleType="centerCrop"
        android:layout_below="@+id/action_bar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="175dp"
        android:layout_height="108dp"
        app:srcCompat="@drawable/studyroom"
        android:id="@+id/studyRoomButton"
        android:scaleType="centerCrop"
        android:layout_below="@+id/action_bar"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="175dp"
        android:layout_height="108dp"
        app:srcCompat="@drawable/bathroom"
        android:id="@+id/bathroomButton"
        android:scaleType="centerCrop"
        android:layout_below="@+id/studyRoomButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="175dp"
        android:layout_height="108dp"
        app:srcCompat="@drawable/livingroom"
        android:id="@+id/livingroomButton"
        android:scaleType="centerCrop"
        android:layout_below="@+id/kitchenButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/action_bar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#e1e1e1"
        android:elevation="4dp"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
